{
  "name": "doc-marker",
  "version": "0.7.0-dev",
  "description": "Annotation tool for creating training data for the text-to-form information retrieval NLP task",
  "author": "Jiří Mayer <mayer@ufal.mff.cuni.cz> (https://ufal.mff.cuni.cz/jiri-mayer)",
  "engines": {
    "parcel": "2.x"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "library": {
      "isLibrary": true,
      "outputFormat": "commonjs",
      "distDir": "dist",
      "includeNodeModules": false
    },
    "development": {
      "context": "browser",
      "source": "development/index.html",
      "publicUrl": "./"
    }
  },
  "scripts": {
    "start": "rm -rf dist .parcel-cache && parcel --target development",
    "build": "rm -rf dist .parcel-cache && parcel build --target library --target types",
    "prettier-check": "prettier src/**/*.ts src/**/*.tsx --check",
    "prettier-write": "prettier src/**/*.ts src/**/*.tsx --write",
    "prepare": "npm run build"
  },
  "dependencies": {
    "buffer": "^6.0.0",
    "lodash": "^4.0.0",
    "moment": "^2.0.0",
    "process": "^0.11.0",
    "quill": "^1.3.7",
    "quill-delta": "^5.1.0",
    "sharp": "^0.34.0",
    "uuid": "^11.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@jsonforms/core": "^3.0.0",
    "@jsonforms/material-renderers": "^3.5.0",
    "@jsonforms/react": "^3.0.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@mui/x-date-pickers": "^7.0.0",
    "i18next": "^25.0.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^15.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@jsonforms/core": "^3.0.0",
    "@jsonforms/material-renderers": "^3.5.0",
    "@jsonforms/react": "^3.0.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@mui/x-date-pickers": "^7.0.0",
    "i18next": "^25.0.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^15.0.0",
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-sass": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "parcel": "^2.8.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
