{
  "name": "doc-marker",
  "version": "0.5.0-dev",
  "description": "Annotation tool for creating training data for the text-to-form information retrieval NLP task",
  "author": "Jiří Mayer <mayer@ufal.mff.cuni.cz> (https://ufal.mff.cuni.cz/jiri-mayer)",
  "engines": {
    "parcel": "2.x"
  },
  "source": "src/index.js",
  "main": "dist/index.js",
  "targets": {
    "library": {
      "isLibrary": true,
      "outputFormat": "commonjs",
      "distDir": "dist",
      "includeNodeModules": false
    },
    "development": {
      "context": "browser",
      "source": "development/index.html",
      "publicUrl": "./"
    }
  },
  "scripts": {
    "start": "rm -rf dist .parcel-cache && parcel --target development",
    "build": "rm -rf dist .parcel-cache && parcel build --target library",
    "prepare": "npm run build"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "process": "^0.11.10",
    "quill": "^1.3.6",
    "quill-delta": "^5.1.0",
    "sharp": "^0.32.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@jsonforms/core": "^3.0.0",
    "@jsonforms/material-renderers": "^3.0.0",
    "@jsonforms/react": "^3.0.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-date-pickers": "^6.0.0",
    "i18next": "^23.0.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^13.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@jsonforms/core": "^3.0.0",
    "@jsonforms/material-renderers": "^3.0.0",
    "@jsonforms/react": "^3.0.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.3",
    "@mui/x-date-pickers": "^6.0.4",
    "@parcel/transformer-sass": "^2.8.2",
    "i18next": "^23.2.3",
    "jotai": "^2.2.1",
    "parcel": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1"
  }
}
